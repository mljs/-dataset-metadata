{
  "name": "ml-dataset-metadata",
  "version": "0.3.0",
  "description": "a class to manipulate metadata for dataset",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c && cheminfo-build --entry src/index.js",
    "prepublishOnly": "rollup -c",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/dataset-metadata.git"
  },
  "keywords": [],
  "author": "Julien Wist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mljs/dataset-metadata/issues"
  },
  "homepage": "https://github.com/mljs/dataset-metadata#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "cheminfo-build": "^1.1.3",
    "eslint": "^7.6.0",
    "eslint-config-cheminfo": "^5.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "jest-matcher-deep-close-to": "^2.0.1",
    "ml-dataset-iris": "^1.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.24.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "ml-matrix": "^6.5.1"
  }
